                                "designer|userClassName": "onMypanel6Initialize",
                                "fn": "onMypanel6Initialize",
                                "implHandler": [
                                    "Ext.getCmp('maindata').setMasked(\r",
                                    "{\r",
                                    "    xtype:'loadmask',\r",
                                    "    message:'Awaiting NFC Tag in (Patient)',\r",
                                    "    fullscreen: true,\r",
                                    "     html:\"<img src='images/loading3.gif'/>\",\r",
                                    "    indicator:false\r",
                                    "});\r",
                                    "\r",
                                    "\r",
                                    "setTimeout(function () {            \r",
                                    "    Ext.getCmp('maindata').setMasked(false);\r",
                                    "\r",
                                    "\r",
                                    "    /*stopwatch starts function*/\r",
                                    "\r",
                                    "    stopWatchInt =  setInterval(function() {\r",
                                    "\r",
                                    "        var elapseTime;\r",
                                    "        var watchObj = Ext.getCmp('timer');\r",
                                    "        var minSec = watchObj.getHtml().split(\":\");\r",
                                    "        var min  = parseInt(minSec[1],10);\r",
                                    "        var sec  = parseInt(minSec[2],10);\r",
                                    "        var msec = parseInt(minSec[3],10);\r",
                                    "        var hr = parseInt(minSec[0],10);\r",
                                    "\r",
                                    "\r",
                                    "        sec++;\r",
                                    "\r",
                                    "        if ( sec  == 60 ) { min++;  sec = 0; }\r",
                                    "        if ( min  == 60 ) { hr++;  min = 0; }\r",
                                    "\r",
                                    "        elapseTime  = (hr  < 10) ?  \"0\" + hr  : hr;\r",
                                    "        elapseTime += (min  < 10) ? \":0\" + min  : \":\" + min;\r",
                                    "        elapseTime += (sec < 10) ? \":0\" + sec : \":\" + sec;\r",
                                    "\r",
                                    "\r",
                                    "        watchObj.setHtml(elapseTime); \r",
                                    "\r",
                                    "    }, 1000);\r",
                                    "\r",
                                    "    /*ends stopwatch starts function*/\r",
                                    "\r",
                                    "\r",
                                    "}, 5000);\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    ""
                                ],
